modules = ["java-graalvm22.3", "python-3.11", "nodejs-22"]
[agent]
expertMode = true
integrations = ["python_anthropic_ai_integrations:1.0.0", "python_openai_ai_integrations:1.0.0", "python_gemini_ai_integrations:1.0.0", "python_openrouter_ai_integrations:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "MineClaw Server"

[[workflows.workflow]]
name = "MineClaw Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "bash start-all.sh"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 25575
externalPort = 3002

[nix]
channel = "stable-25_05"
packages = ["jdk21", "scc"]

[deployment]
run = ["bash", "start-all.sh"]
deploymentTarget = "vm"

